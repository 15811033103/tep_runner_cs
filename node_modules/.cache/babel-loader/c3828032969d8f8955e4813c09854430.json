AAzC,KAAA,CAAA,IAAA,CAAK,I,GAAI,M;kBAAgC;AAA9B,gBAAA,KAAkB,EAAlB;AAAA,2BAAA;AAAA;AAA8B,eAA5D,E,IAAA,E;;AAAA,gB,cAAA,CAA4D,CAA5D;AAAA,a,CADuB;;;;AAAA,WAAzB,CAEe,EACf,YAAA,CAEe,uBAFf,EAEe;AAFD,YAAA,KAAK,EAAC;AAEL,WAFf,EAAwB;8BACtB;AAAA,qBAAgE,CAAhE,YAAA,CAAgE,mBAAhE,EAAgE;4BAA7C,KAAA,CAAA,IAAA,CAAK,QAAwC;;yBAA7C,KAAA,CAAA,IAAA,CAAK,Q,GAAQ,M;kBAAgC;AAA9B,gBAAA,KAAkB,EAAlB;AAAA,2BAAA;AAAA;AAA8B,eAAhE,E,IAAA,E;;AAAA,gB,cAAA,CAAgE,CAAhE;AAAA,a,CADsB;;;;AAAA,WAAxB,CADe,EAIf,YAAA,CAKe,uBALf,EAKe;AALD,YAAA,KAAK,EAAC;AAKL,WALf,EAAwB;8BACtB;AAAA,qBAA2D,CAA3D,YAAA,CAA2D,mBAA3D,EAA2D;4BAAxC,KAAA,CAAA,IAAA,CAAK,GAAmC;;yBAAxC,KAAA,CAAA,IAAA,CAAK,G,GAAG,M;kBAAgC;AAA9B,gBAAA,KAAkB,EAAlB;AAAA,2BAAA;AAAA;AAA8B,eAA3D,E,IAAA,E;;AAAA,gB,cAAA,CAA2D,EAC3D,mBAAA,CAAA,MAAA,CAD2D,EAE3D,YAAA,CAAmD,mBAAnD,EAAmD;4BAAhC,KAAA,CAAA,IAAA,CAAK,GAA2B;;yBAAhC,KAAA,CAAA,IAAA,CAAK,G,GAAG,M;kBAAwB;AAAtB,gBAAA,KAAK,EAAC;AAAgB,eAAnD,E;kCAAuC;AAAA,yBAAC,C,WAAA,CAAD;AAAA,iB;;;;eAAvC,E;;AAAA,gB,cAAA,CAF2D,EAG3D,YAAA,CAAmD,mBAAnD,EAAmD;4BAAhC,KAAA,CAAA,IAAA,CAAK,GAA2B;;yBAAhC,KAAA,CAAA,IAAA,CAAK,G,GAAG,M;kBAAwB;AAAtB,gBAAA,KAAK,EAAC;AAAgB,eAAnD,E;kCAAuC;AAAA,yBAAC,C,WAAA,CAAD;AAAA,iB;;;;eAAvC,E;;AAAA,gB,cAAA,CAH2D,CAA3D;AAAA,a,CADsB;;;;AAAA,WAAxB,CAJe,EAWf,YAAA,CAEe,uBAFf,EAEe;AAFD,YAAA,KAAK,EAAC;AAEL,WAFf,EAAwB;8BACtB;AAAA,qBAA+E,CAA/E,YAAA,CAA+E,mBAA/E,EAA+E;AAArE,gBAAA,IAAI,EAAC,UAAgE;4BAA5C,KAAA,CAAA,IAAA,CAAK,OAAuC;;yBAA5C,KAAA,CAAA,IAAA,CAAK,O,GAAO,M;kBAAgC;AAA9B,gBAAA,KAAkB,EAAlB;AAAA,2BAAA;AAAA;AAA8B,eAA/E,E,IAAA,E;;AAAA,gB,cAAA,CAA+E,CAA/E;AAAA,a,CADsB;;;;AAAA,WAAxB,CAXe,CAFf;AAAA,S;;;;OAFF,E;;AAAA,Q,SAAA,CAkBU,CAlBV;AAAA,K;;;;GAHF,E;;AAAA,I,cAAA,CA+BI,CA5CN,CA6CI,CAhGN,C","sourcesContent":["<template>\n  <div class=\"home\">\n    <!--style=\"margin: 2px 1px\" = 上下俩个像素，左右1个像素 -->\n    <!--新增、导入、导出 按钮 -->\n    <div style=\"margin: 2px 1px\">\n      <!--type=\"primary\" = 是重要的颜色，还有其他警告、危险代表不同颜色 -->\n      <el-button type=\"primary\" @click=\"add\">新增</el-button>\n      <el-button type=\"primary\">导入</el-button>\n      <el-button type=\"primary\">导出</el-button>\n\n    </div>\n    <!--查询输入框 -->\n    <div>\n      <!-- style=\"width: 20%\"  input输入框 长度-->\n      <el-input v-model=\"search\" placeholder=\"请输出查询内容\" style=\"width: 20%\" clearable/>\n      <el-button type=\"primary\" @click=\"load\">查询</el-button>\n    </div>\n\n    <!-- 表格-->\n    <el-table :data=\"tableData\" border style=\"width: 100%\">\n      <!--sortable 是排序 -->\n      <el-table-column prop=\"id\" label=\"日期\" sortable  />\n\n      <el-table-column prop=\"name\" label=\"名称\"  />\n      <el-table-column prop=\"nickname\" label=\"昵称\"  />\n      <el-table-column prop=\"sex\" label=\"性别\"  />\n      <el-table-column prop=\"address\" label=\"地址\" />\n\n      <!-- 固定》右边操作按钮-->\n      <!-- 切记#default=\"scope\"-->\n      <el-table-column fixed=\"right\" label=\"操作 \" width=\"120\">\n        <template #default=\"scope\">\n          <!-- size=\"small\" == 按钮大小-->\n          <!-- type=\"text\" == 按钮文本类型-->\n          <el-button type=\"text\" size=\"small\" @click=\"handleEdit(scope.row)\">编辑</el-button>\n\n          <el-popconfirm title=\"Are you sure to delete this?\" @confirm=\"handleDelete(scope.row.id)\">\n\n            <!-- 因为点击删除会触发确认操作弹窗，所以@click=\"handleDelete(scope.row.id)\" 绑定函数放到确认组件上-->\n            <template #reference>\n              <el-button type=\"text\" size=\"small\" >删除</el-button>\n            </template>\n          </el-popconfirm>\n\n        </template>\n      </el-table-column>\n\n    </el-table>\n\n\n    <!--style=\"margin: 2px 1px\" = 上下俩个像素，左右1个像素 -->\n    <!--sizes(可选择多少条作为一页)，page-size（默认20条=一页） -->\n    <div style=\"margin: 10px 0\">\n      <el-pagination\n              v-model:currentPage=\"currentPage\"\n              :page-sizes=\"[10, 20, 30, 40]\"\n              :page-size=\"pageSize\"\n              layout=\"total, sizes, prev, pager, next, jumper\"\n              :total=\"total\"\n              @size-change=\"handleSizeChange\"\n              @current-change=\"handleCurrentChange\"\n      >\n      </el-pagination>\n\n      <!--对话框 -->\n      <el-dialog v-model=\"dialogVisible\" title=\"弹窗\" width=\"30%\" >\n\n        <!-- from 表单-->\n        <el-form ref=\"form\" :model=\"form\" label-width=\"120px\">\n\n          <el-form-item label=\"用户名\">\n            <el-input v-model=\"form.name\" style=\"width: 80%\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"昵称\">\n            <el-input v-model=\"form.nickname\" style=\"width: 80%\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"性别\">\n            <el-input v-model=\"form.sex\" style=\"width: 80%\"></el-input>\n            <!-- 单选框-->\n            <el-radio v-model=\"form.sex\" label=\"1\">男</el-radio>\n            <el-radio v-model=\"form.sex\" label=\"2\">女</el-radio>\n          </el-form-item>\n\n          <el-form-item label=\"地址\">\n            <el-input type=\"textarea\" v-model=\"form.address\" style=\"width: 80%\"></el-input>\n          </el-form-item>\n        </el-form>\n\n\n        <template #footer>\n      <span class=\"dialog-footer\">\n        <el-button @click=\"dialogVisible = false\">取消</el-button>\n        <el-button type=\"primary\" @click=\"save \">确定</el-button>\n      </span>\n        </template>\n      </el-dialog>\n    </div>\n  </div>\n</template>\n\n<script>\n  // @ is an alias to /src\n\n  import axios from \"axios\";\n\n  export default {\n    name: 'Home',\n    data(){\n      return{\n        form:{},\n        dialogVisible: false,\n        search: \"\",\n        currentPage:1,\n        total:50,\n        pageSize:10,\n        tableData: [\n\n        ],\n      }\n    },\n    created() {\n      this.load()\n    },\n    methods:{\n      add(){\n        this.dialogVisible = true\n        this.form = {}\n      },\n      load(){\n        axios({url: \"http://127.0.0.1:5000/userinfo\",\n          method: 'get',\n          params:{\n            \"sizeNum\":this.pageSize,\n            \"pageNum\":this.currentPage,\n            \"search\":this.search}}).then(res =>{\n          console.log(res.data)\n          this.tableData = res.data.interface\n        })\n      },\n      save(){\n        //if (this.form.id)判断form中是否存在id，如果存在调用编辑接口，如果不存在调用新增接口\n        // if判断接口返回是否是200，如果是说明保存成功，否则保存失败，页面弹窗展示\n        axios({url:\"http://127.0.0.1:5000/userinfo\",\n          method:'post',\n          params:this.form}).then((res =>{\n\n          if (res.data.code == '200'){\n            console.log(\"adsdas\")\n            this.$messageBox({\n              type:\"成功\",\n              message:\"保存成功\"\n            })\n          } else {\n            this.$messageBox({\n              type:\"error\",\n              message:\"保存失败\"\n            })\n          }\n          this.load() //刷新表格的数据\n          this.dialogVisible = false //关闭弹窗\n        }))},\n      handleEdit(row){\n        this.form = JSON.parse(JSON.stringify(row))\n        this.dialogVisible = true\n      },\n      handleDelete(id){\n        console.log(id)\n      },\n      handleSizeChange(pageSize){ //改变当前每夜的个数触发\n        this.pageSize = pageSize\n        this.load()\n\n      },\n      handleCurrentChange(currentPage){ //改变当前页码触发\n        this.currentPage = currentPage\n        this.load()\n      }\n    },\n    components: {\n    }\n  }\n</script>\n"],"sourceRoot":""}]}