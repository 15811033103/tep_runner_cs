qB,IAArB;AACA,WAAK,IAAL,GAAY,EAAZ;AACD,KAJK;;AAKN,IAAA,IAAI,GAAE;AACJ,MAAA,KAAK,CAAC;AAAC,QAAA,GAAG,EAAE,gCAAN;AACJ,QAAA,MAAM,EAAE,KADJ;AAEJ,QAAA,MAAM,EAAC;AACL,qBAAU,KAAK,QADV;AAEL,qBAAU,KAAK,WAFV;AAGL,oBAAS,KAAK;AAHT;AAFH,OAAD,CAAL,CAK4B,IAL5B,CAKiC,GAAE,IAAI;AACrC,QAAA,OAAO,CAAC,GAAR,CAAY,GAAG,CAAC,IAAhB;AACA,aAAK,SAAL,GAAiB,GAAG,CAAC,IAAJ,CAAS,SAA1B;AACD,OARD;AASD,KAfK;;AAgBN,IAAA,IAAI,GAAE;AACJ;AACA;AACA,MAAA,KAAK,CAAC;AAAC,QAAA,GAAG,EAAC,gCAAL;AACJ,QAAA,MAAM,EAAC,MADH;AAEJ,QAAA,MAAM,EAAC,KAAK;AAFR,OAAD,CAAL,CAEqB,IAFrB,CAE2B,GAAE,IAAI;AAE/B,YAAI,GAAG,CAAC,IAAJ,CAAS,IAAT,IAAiB,KAArB,EAA2B;AACzB,UAAA,OAAO,CAAC,GAAR,CAAY,QAAZ;AACA,eAAK,WAAL,CAAiB;AACf,YAAA,IAAI,EAAC,IADU;AAEf,YAAA,OAAO,EAAC;AAFO,WAAjB;AAIF,SANA,MAMO;AACL,eAAK,WAAL,CAAiB;AACf,YAAA,IAAI,EAAC,OADU;AAEf,YAAA,OAAO,EAAC;AAFO,WAAjB;AAIF;;AACA,aAAK,IAAL,GAd+B,CAcnB;;AACZ,aAAK,aAAL,GAAqB,KAArB,CAf+B,CAeJ;AAC5B,OAlBD;AAkBI,KArCA;;AAsCN,IAAA,UAAU,CAAC,GAAD,EAAK;AACb,WAAK,IAAL,GAAY,IAAI,CAAC,KAAL,CAAW,IAAI,CAAC,SAAL,CAAe,GAAf,CAAX,CAAZ;AACA,WAAK,aAAL,GAAqB,IAArB;AACD,KAzCK;;AA0CN,IAAA,YAAY,CAAC,EAAD,EAAI;AACd,MAAA,OAAO,CAAC,GAAR,CAAY,EAAZ;AACD,KA5CK;;AA6CN,IAAA,gBAAgB,CAAC,QAAD,EAAU;AAAE;AAC1B,WAAK,QAAL,GAAgB,QAAhB;AACA,WAAK,IAAL;AAED,KAjDK;;AAkDN,IAAA,mBAAmB,CAAC,WAAD,EAAa;AAAE;AAChC,WAAK,WAAL,GAAmB,WAAnB;AACA,WAAK,IAAL;AACF;;AArDM,GAlBK;AAyEb,EAAA,UAAU,EAAE;AAzEC,CAAf","sourcesContent":["<template>\n  <div class=\"home\">\n    <!--style=\"margin: 2px 1px\" = 上下俩个像素，左右1个像素 -->\n    <!--新增、导入、导出 按钮 -->\n    <div style=\"margin: 2px 1px\">\n      <!--type=\"primary\" = 是重要的颜色，还有其他警告、危险代表不同颜色 -->\n      <el-button type=\"primary\" @click=\"add\">新增</el-button>\n      <el-button type=\"primary\">导入</el-button>\n      <el-button type=\"primary\">导出</el-button>\n\n    </div>\n    <!--查询输入框 -->\n    <div>\n      <!-- style=\"width: 20%\"  input输入框 长度-->\n      <el-input v-model=\"search\" placeholder=\"请输出查询内容\" style=\"width: 20%\" clearable/>\n      <el-button type=\"primary\" @click=\"load\">查询</el-button>\n    </div>\n\n    <!-- 表格-->\n    <el-table :data=\"tableData\" border style=\"width: 100%\">\n      <!--sortable 是排序 -->\n      <el-table-column prop=\"id\" label=\"日期\" sortable  />\n\n      <el-table-column prop=\"name\" label=\"名称\"  />\n      <el-table-column prop=\"nickname\" label=\"昵称\"  />\n      <el-table-column prop=\"sex\" label=\"性别\"  />\n      <el-table-column prop=\"address\" label=\"地址\" />\n\n      <!-- 固定》右边操作按钮-->\n      <!-- 切记#default=\"scope\"-->\n      <el-table-column fixed=\"right\" label=\"操作 \" width=\"120\">\n        <template #default=\"scope\">\n          <!-- size=\"small\" == 按钮大小-->\n          <!-- type=\"text\" == 按钮文本类型-->\n          <el-button type=\"text\" size=\"small\" @click=\"handleEdit(scope.row)\">编辑</el-button>\n\n          <el-popconfirm title=\"Are you sure to delete this?\" @confirm=\"handleDelete(scope.row.id)\">\n\n            <!-- 因为点击删除会触发确认操作弹窗，所以@click=\"handleDelete(scope.row.id)\" 绑定函数放到确认组件上-->\n            <template #reference>\n              <el-button type=\"text\" size=\"small\" >删除</el-button>\n            </template>\n          </el-popconfirm>\n\n        </template>\n      </el-table-column>\n\n    </el-table>\n\n\n    <!--style=\"margin: 2px 1px\" = 上下俩个像素，左右1个像素 -->\n    <!--sizes(可选择多少条作为一页)，page-size（默认20条=一页） -->\n    <div style=\"margin: 10px 0\">\n      <el-pagination\n              v-model:currentPage=\"currentPage\"\n              :page-sizes=\"[10, 20, 30, 40]\"\n              :page-size=\"pageSize\"\n              layout=\"total, sizes, prev, pager, next, jumper\"\n              :total=\"total\"\n              @size-change=\"handleSizeChange\"\n              @current-change=\"handleCurrentChange\"\n      >\n      </el-pagination>\n\n      <!--对话框 -->\n      <el-dialog v-model=\"dialogVisible\" title=\"弹窗\" width=\"30%\" >\n\n        <!-- from 表单-->\n        <el-form ref=\"form\" :model=\"form\" label-width=\"120px\">\n\n          <el-form-item label=\"用户名\">\n            <el-input v-model=\"form.name\" style=\"width: 80%\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"昵称\">\n            <el-input v-model=\"form.nickname\" style=\"width: 80%\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"性别\">\n            <el-input v-model=\"form.sex\" style=\"width: 80%\"></el-input>\n            <!-- 单选框-->\n            <el-radio v-model=\"form.sex\" label=\"1\">男</el-radio>\n            <el-radio v-model=\"form.sex\" label=\"2\">女</el-radio>\n          </el-form-item>\n\n          <el-form-item label=\"地址\">\n            <el-input type=\"textarea\" v-model=\"form.address\" style=\"width: 80%\"></el-input>\n          </el-form-item>\n        </el-form>\n\n\n        <template #footer>\n      <span class=\"dialog-footer\">\n        <el-button @click=\"dialogVisible = false\">取消</el-button>\n        <el-button type=\"primary\" @click=\"save \">确定</el-button>\n      </span>\n        </template>\n      </el-dialog>\n    </div>\n  </div>\n</template>\n\n<script>\n  // @ is an alias to /src\n\n  import axios from \"axios\";\n\n  export default {\n    name: 'Home',\n    data(){\n      return{\n        form:{},\n        dialogVisible: false,\n        search: \"\",\n        currentPage:1,\n        total:50,\n        pageSize:10,\n        tableData: [\n\n        ],\n      }\n    },\n    created() {\n      this.load()\n    },\n    methods:{\n      add(){\n        this.dialogVisible = true\n        this.form = {}\n      },\n      load(){\n        axios({url: \"http://127.0.0.1:5000/userinfo\",\n          method: 'get',\n          params:{\n            \"sizeNum\":this.pageSize,\n            \"pageNum\":this.currentPage,\n            \"search\":this.search}}).then(res =>{\n          console.log(res.data)\n          this.tableData = res.data.interface\n        })\n      },\n      save(){\n        //if (this.form.id)判断form中是否存在id，如果存在调用编辑接口，如果不存在调用新增接口\n        // if判断接口返回是否是200，如果是说明保存成功，否则保存失败，页面弹窗展示\n        axios({url:\"http://127.0.0.1:5000/userinfo\",\n          method:'post',\n          params:this.form}).then((res =>{\n\n          if (res.data.code == '200'){\n            console.log(\"adsdas\")\n            this.$messageBox({\n              type:\"成功\",\n              message:\"保存成功\"\n            })\n          } else {\n            this.$messageBox({\n              type:\"error\",\n              message:\"保存失败\"\n            })\n          }\n          this.load() //刷新表格的数据\n          this.dialogVisible = false //关闭弹窗\n        }))},\n      handleEdit(row){\n        this.form = JSON.parse(JSON.stringify(row))\n        this.dialogVisible = true\n      },\n      handleDelete(id){\n        console.log(id)\n      },\n      handleSizeChange(pageSize){ //改变当前每夜的个数触发\n        this.pageSize = pageSize\n        this.load()\n\n      },\n      handleCurrentChange(currentPage){ //改变当前页码触发\n        this.currentPage = currentPage\n        this.load()\n      }\n    },\n    components: {\n    }\n  }\n</script>\n"],"sourceRoot":""}]}